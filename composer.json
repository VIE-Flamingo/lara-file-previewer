{
    "name": "khutachan/lara-file-previewer",
    "description": "A streamlined file previewer for Laravel projects.",
    "keywords": [
        "khutachan",
        "lara-file-previewer",
        "file previewer",
        "laravel"
    ],
    "homepage": "https://github.com/khutachan/lara-file-previewer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Khutachan",
            "email": "your-email@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Khutachan\\LaraFilePreviewer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Khutachan\\LaraFilePreviewer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khutachan\\LaraFilePreviewer\\LaraFilePreviewerServiceProvider"
            ],
            "aliases": {
                "LaraFilePreviewer": "Khutachan\\LaraFilePreviewer\\LaraFilePreviewerFacade"
            }
        }
    }
}